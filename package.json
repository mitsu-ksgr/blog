{
  "name": "mitsu-note",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin=prettier-plugin-svelte --check ./**/*.{js,ts,svelte,css} && eslint --fix --ext .js,.ts,.svelte src",
    "format": "prettier --plugin=prettier-plugin-svelte --write ./**/*.{js,ts,svelte,css}",
    "make-catalogs": "yarn tsx src/lib/task/cataloger.ts",
    "new-post": "yarn tsx src/lib/task/post.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/marked": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.5.1",
    "feed": "^5.0.0",
    "front-matter": "^4.0.2",
    "highlight.js": "^11.11.1",
    "marked": "^15.0.11",
    "marked-highlight": "^2.2.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.88.0",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "type": "module"
}
